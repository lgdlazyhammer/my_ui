<script>
function onSelectChangeSelectedIndex(itemSelected){
	this.selectedIndex = itemSelected;
	console.log("selected index  " + this.selectedIndex);
	
}

function onSelectedChangeTitle(itemSelected){
	this.title = itemSelected;
	console.log("selected title " + this.title);
}
</script>

