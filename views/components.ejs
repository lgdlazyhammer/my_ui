<h2>select onclick 事件</h2>
<hr/>
<h3>select onclick 事件 无value</h3>
<select onchange="onSelectChangeSelectedIndex(this.options[this.options.selectedIndex].value),onSelectedChangeTitle(this.options[this.options.selectedIndex].value)" value="">
  <option>Volvo</option>
  <option>Saab</option>
  <option>Mercedes</option>
  <option>Audi</option>
</select>

<hr/>
<h3>select onclick 事件 有 value</h3>
<select onchange="onSelectChangeSelectedIndex(this.options[this.options.selectedIndex].value),onSelectedChangeTitle(this.options[this.options.selectedIndex].value)" value="">
  <option value="1">Volvo</option>
  <option value="2">Saab</option>
  <option value="3">Mercedes</option>
  <option value="4">Audi</option>
</select>

